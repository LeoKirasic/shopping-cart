{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": ["plugin:react/recommended", "eslint:recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest"
    },
    "plugins": ["react"],
    "rules": {
      "no-unused-vars": "warn"
    }
  }